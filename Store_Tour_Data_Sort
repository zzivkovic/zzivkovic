Sub Sort_by_store_number()
'
' Sort_by_store_number Macro
'

'
    Range("O15:Q15").Select
    Range(Selection, Selection.End(xlDown)).Select
    ActiveWorkbook.Worksheets("YTD BY STORE").Sort.SortFields.Clear
    ActiveWorkbook.Worksheets("YTD BY STORE").Sort.SortFields.Add Key:=Range( _
        "P15:P49"), SortOn:=xlSortOnValues, Order:=xlAscending, DataOption:= _
        xlSortNormal
    With ActiveWorkbook.Worksheets("YTD BY STORE").Sort
        .SetRange Range("O15:Q49")
        .Header = xlGuess
        .MatchCase = False
        .Orientation = xlTopToBottom
        .SortMethod = xlPinYin
        .Apply
    End With
    Range("A1:H1").Select
End Sub
